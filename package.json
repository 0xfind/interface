  "scripts": {
    "contracts:compile": "typechain --target ethers-v5 --out-dir src/abis/types \"./src/abis/**/*.json\"",
    "graphql:generate:uniswap": "graphql-codegen --config scripts/codegen/uniswap.yml",
    "graphql:generate:find": "graphql-codegen --config scripts/codegen/find.yml",
    "graphql:generate:blocknumber": "graphql-codegen --config scripts/codegen/blocknumber.yml",
    "graphql:generate": "yarn graphql:generate:uniswap && yarn graphql:generate:find && yarn graphql:generate:blocknumber",
    "openapi:generate:user": "npx @rtk-query/codegen-openapi scripts/codegen/service/user.ts",
    "openapi:generate:signature": "npx @rtk-query/codegen-openapi scripts/codegen/service/signature.ts",
    "openapi:generate:claim": "npx @rtk-query/codegen-openapi scripts/codegen/service/claim.ts",
    "openapi:generate": "yarn openapi:generate:user && yarn openapi:generate:signature && yarn openapi:generate:claim",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "prebuild": "yarn i18n:compile",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "i18n:extract": "lingui extract --clean",
    "i18n:compile": "yarn i18n:extract && lingui compile",
    "postinstall": "yarn contracts:compile && yarn graphql:generate && yarn openapi:generate && yarn i18n:compile"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
